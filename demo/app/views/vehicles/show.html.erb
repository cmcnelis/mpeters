<% content_for :title=>"Vehicle" %>

<div class="row">
    <div class="page-header">
        <h1>Vehicle - <small><%= @vehicle.vin %></small></h1>
    </div>
</div>

<% if !@vehicle.covered %>
    <div class="row">
        <div class="span12 alert">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <strong>Warning!</strong> Vehicle deductible isn't currently covered.
        </div>
    </div>
<% end %>
