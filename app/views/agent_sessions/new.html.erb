<div class="container">
  <div class="page-header">
    <h1>Login In</h1>
  </div>
</div>
<div class="container">
<%= form_for @agent_session  do |f| %>
  <%= show_errors(@agent_session) %>
  <%= f.text_field :email , :size=>25 , :placeholder=>'Email address' %><br><br>
  <%= f.password_field :password, :size=> 25 , :placeholder=>"Password" %><br><br>
  <%= f.submit "Login" , :class=>"btn btn-primary"%>
<% end %>
</div>
