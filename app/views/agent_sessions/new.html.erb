<%= form_for @agent_session,  url: agent_session_path do |f| %>
  <% if @agent_session.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@agent_session.errors.count, "error") %> prohibited:</h2>
    <ul>
      <% @agent_session.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
  <% end %>
  <%= f.label :email%><br />
  <%= f.text_field :email %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.submit "Login" %>
<% end %>
