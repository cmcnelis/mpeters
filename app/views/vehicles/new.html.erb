<% content_for :title, 'New Vehicle' %>

<div class='container'>
    <div class="panel panel-default">
        <div class='panel-heading'>
            <h3>New Vehicle</h3>
        </div>
        <div class='panel-body'>
            <%= form_for @vehicle, :url=>account_policy_vehicles_path do |f| %>
            <%= show_errors(@vehicle) %>
            <%= render :partial=>'form', :locals=>{:form=>f }%>
            <% end %>
        </div>
    </div>
</div>

