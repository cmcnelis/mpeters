<% content_for :title, "Edit Policy" %>
<div class="container">
    <div class="page-header">
        <h1>Edit Policy</h1>
    </div>
</div>

<%= form_for @policy, :url=>account_policy_path do |f| %>
    <%= render :partial=>'form', :object=>f %>
    <div class="container">
        <%= f.submit 'Submit', :class=>'btn btn-primary' %>
<%= link_to 'Cancel', :back, :class=>'btn btn-primary', :confirm=>"Are you sure?" %>
    </div
<% end %>



