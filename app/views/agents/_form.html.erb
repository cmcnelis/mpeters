<%= show_errors(@agent) %>
<fieldset>
<div class="form-group">
        <%= form.label :first_name, "First Name", :class=>'col-lg-2 control-label'%>
    <div class="col-lg-10">
        <%= form.text_field :first_name ,:class=>'form-control', :placeholder=>'First Name'%>
    </div>
</div>
<div class="form-group">
        <%= form.label :last_name, "Last Name" ,:class=>'col-lg-2 control-label' %>
    <div class='col-lg-10'>
        <%= form.text_field :last_name, :class=>'form-control', :placeholder=>'Last Name'%>
    </div>
</div>
<div class="form-group">
    <%= form.label :company , :class=>'col-lg-2 control-label'%>
    <div class='col-lg-10'>
        <%= form.text_field :company, :class=>'form-control', :placeholder=>'Company Name' %>
    </div>
</div>
<div class='form-group'>
    <%= form.label :email , :class=>'control-label col-lg-2'%>
    <div class='col-lg-10'>
        <%= form.email_field :email , :class=>'form-control', :placeholder=>'Email'%>
    </div>
</div>
<div class='form-group'>
    <%= form.label :email_confirmation,"Confirm Email", :class=>'control-label col-lg-2'%>
    <div class='col-lg-10'>
        <%= form.email_field :email_confirmation, :class=>'form-control' %>
    </div>
</div>
<div class='form-group'>
    <%= form.label :password, :class=>'control-label col-lg-2' %>
    <div class='col-lg-10'>
        <%= form.password_field :password, :class=>'form-control', :placeholder=>"Password" %>
    </div>
</div>
<div class='form-group'>
    <%= form.label  :password_confirmation, 'Confirm Password', :class=>'col-lg-2 control-label'%>
    <div class="col-lg-10">
        <%= form.password_field :password_confirmation, :class=>'form-control' %>
    </div>
</div>
<div class="form-group">
    <div class="col-lg-10 col-offset-2">
        <%= form.submit "Submit", :class=>"btn btn-success btn-sm" %>
        <%= link_to "Cancel", :back,  :class=> "btn btn-danger btn-sm", :confirm=>"Are you sure?" %>
    </div>
</div>
</fieldset>


